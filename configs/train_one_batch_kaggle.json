{
    "name": "rawnet2",
    "n_gpu": 1,
    "arch": {
        "type": "RawNet2Model",
        "args": {
            "sinc_channels": 128,
            "sinc_filter_length": 1024,
            "channels1": 20,
            "channels2": 128
        }
    },
    "data": {
        "train": {
            "batch_size": 8,
            "num_workers": 1,
            "datasets": [
                {
                    "type": "ASVspoof2019Dataset",
                    "args": {
                        "dir": "/kaggle/input/asvpoof-2019-dataset",
                        "part": "train",
                        "max_sec_length": 3,
                        "limit": 8
                    }
                }
            ]
        },
        "val": {
            "batch_size": 1,
            "num_workers": 1,
            "datasets": [
                {
                    "type": "ASVspoof2019Dataset",
                    "args": {
                        "dir": "/kaggle/input/asvpoof-2019-dataset",
                        "part": "dev",
                        "limit": 1
                    }
                }
            ]
        },
        "test": {
            "batch_size": 1,
            "num_workers": 1,
            "datasets": [
                {
                    "type": "ASVspoof2019Dataset",
                    "args": {
                        "dir": "/kaggle/input/asvpoof-2019-dataset",
                        "part": "eval",
                        "limit": 1
                    }
                }
            ]
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 3e-4,
            "weight_decay": 1e-4
        }
    },
    "loss": {
        "type": "RawNet2Loss",
        "args": {}
    },
    "metrics":[
        {
            "type": "EqualErrorRate",
            "args": {}
        }
    ],
    "lr_scheduler": {
        "type": "OneCycleLR",
        "args": {
            "epochs": 10,
            "steps_per_epoch": 25,
            "anneal_strategy": "cos",
            "max_lr": 3e-4,
            "pct_start": 0.2
        }
    },
    "trainer": {
        "epochs": 10,
        "len_epoch": 25,
        "log_step": 5,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 100,
        "visualize": "wandb",
        "wandb_project": "dla5_one_batch",
        "grad_norm_clip": 5
    }
}
